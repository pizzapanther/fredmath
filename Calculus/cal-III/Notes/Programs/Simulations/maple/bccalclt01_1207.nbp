Cell[TextData[{
 StyleBox["THEOREM 12.8", "TheoremFont"],
 "\t",
 StyleBox["Motion with Constant ",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   RowBox[{"\[LeftBracketingBar]", 
    StyleBox["r",
     FontWeight->Bold,
     FontSlant->Plain], "\[RightBracketingBar]"}], TraditionalForm]]],
 "\nLet ",
 Cell[BoxData[
  FormBox[
   StyleBox["r",
    FontWeight->Bold,
    FontSlant->Plain], TraditionalForm]]],
 " describe a path on which ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[LeftBracketingBar]", 
    StyleBox["r",
     FontWeight->Bold,
     FontSlant->Plain], "\[RightBracketingBar]"}], TraditionalForm]]],
 " is constant (motion on a circle or sphere centered at the origin). Then, \
",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     StyleBox["r",
      FontWeight->Bold,
      FontSlant->Plain], "\[CenterDot]", 
     StyleBox["v",
      FontWeight->Bold,
      FontSlant->Plain]}], "=", "0"}], TraditionalForm]]],
 ", which means the position vector and the velocity vector are orthogonal at \
all times for which the functions are defined."
}], "Theorem",
 CellChangeTimes->{{3.476872289217618*^9, 3.4768723552755833`*^9}, {
  3.482443808535182*^9, 3.482443825992915*^9}},
 CellTags->"THEOREM 12.8 Motion with Constant |r|"],

Cell[TextData[{
 StyleBox["Proof", "ProofFont"],
 "  If ",
 Cell[BoxData[
  FormBox[
   StyleBox["r",
    FontWeight->Bold,
    FontSlant->Plain], TraditionalForm]]],
 " has constant magnitude, then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"\[LeftBracketingBar]", 
      RowBox[{
       StyleBox["r",
        FontWeight->Bold,
        FontSlant->Plain], "(", "t", ")"}], "\[RightBracketingBar]"}], "2"], 
    "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       StyleBox["r",
        FontWeight->Bold,
        FontSlant->Plain], "(", "t", ")"}], "\[CenterDot]", 
      RowBox[{
       StyleBox["r",
        FontWeight->Bold,
        FontSlant->Plain], "(", "t", ")"}]}], "=", "c"}]}], TraditionalForm]]],
 " for some constant ",
 Cell[BoxData[
  FormBox["c", TraditionalForm]]],
 ". Differentiating the equation ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      StyleBox["r",
       FontWeight->Bold,
       FontSlant->Plain], "(", "t", ")"}], "\[CenterDot]", 
     RowBox[{
      StyleBox["r",
       FontWeight->Bold,
       FontSlant->Plain], "(", "t", ")"}]}], "=", "c"}], TraditionalForm]]],
 ", we have "

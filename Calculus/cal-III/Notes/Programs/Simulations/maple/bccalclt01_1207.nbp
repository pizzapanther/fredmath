Cell[TextData[{
 StyleBox["c.",
  FontWeight->"Bold"],
 "\tRecall that the area bounded by the polar curve ",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", "=", 
    RowBox[{"f", "(", "\[Theta]", ")"}]}], TraditionalForm]]],
 " on the interval ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{"0", ",", "\[Theta]"}], "]"}], TraditionalForm]]],
 " is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", "(", "\[Theta]", ")"}], "=", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       RowBox[{" ", "0"}], 
       RowBox[{" ", "\[Theta]"}]], 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"f", "(", "u", ")"}], ")"}], "2"], " ", "d", 
       "\[VeryThinSpace]", "u"}]}]}]}], TraditionalForm]]],
 ". Letting ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", 
     RowBox[{"\[Theta]", "(", "t", ")"}], ")"}], "=", 
    RowBox[{"\[LeftBracketingBar]", 
     RowBox[{
      StyleBox["r",
       FontWeight->Bold,
       FontSlant->Plain], "(", 
      RowBox[{"\[Theta]", "(", "t", ")"}], ")"}], "\[RightBracketingBar]"}]}],
    TraditionalForm]]],
 ", show that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"A", "'"}], 
     RowBox[{"(", "t", ")"}]}], "=", 
    RowBox[{
     FractionBox["1", "2"], "a", " ", "b"}]}], TraditionalForm]]],
 "."
}], "SubProblem",
 CellChangeTimes->{{3.476968952538464*^9, 3.4769690267029247`*^9}, {
  3.484233644611329*^9, 3.48423364752185*^9}, {3.500510149924508*^9, 
  3.5005101820593033`*^9}}],


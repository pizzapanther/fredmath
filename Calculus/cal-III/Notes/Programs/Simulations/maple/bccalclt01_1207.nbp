Cell[TextData[{
 StyleBox["65.\tTilted ellipse",
  FontWeight->"Bold"],
 "  Consider the curve ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     StyleBox["r",
      FontWeight->Bold,
      FontSlant->Plain], "(", "t", ")"}], "=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      RowBox[{"cos", " ", "t"}], ",", 
      RowBox[{"sin", " ", "t"}], ",", 
      RowBox[{"c", " ", "sin", " ", "t"}]}], "\[RightAngleBracket]"}]}], 
   TraditionalForm]]],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "\[LessEqual]", "t", "\[LessEqual]", 
    RowBox[{"2", "\[Pi]"}]}], TraditionalForm]]],
 ", where ",
 Cell[BoxData[
  FormBox["c", TraditionalForm]]],
 " is a real number. It can be shown that the curve lies in a plane. Prove \
that the curve is an ellipse in that plane."
}], "Problem",
 CellChangeTimes->{{3.4769688435257397`*^9, 3.476968882886753*^9}, {
  3.482444342329095*^9, 3.48244434293358*^9}}],

Cell[TextData[{
 StyleBox["66.\tEqual area property",
  FontWeight->"Bold"],
 "  Consider the ellipse ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     StyleBox["r",
      FontWeight->Bold,
      FontSlant->Plain], "(", "t", ")"}], "=", 
    RowBox[{"\[LeftAngleBracket]", 
     RowBox[{
      RowBox[{"a", " ", "cos", " ", "t"}], ",", 
      RowBox[{"b", " ", "sin", " ", "t"}]}], "\[RightAngleBracket]"}]}], 
   TraditionalForm]]],
 ", for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "\[LessEqual]", "t", "\[LessEqual]", 
    RowBox[{"2", "\[Pi]"}]}], TraditionalForm]]],
 ", where ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]]],
 " are real numbers. Let ",
 Cell[BoxData[
  FormBox["\[Theta]", TraditionalForm]]],
 " be the angle between the position vector and the ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]]],
 "-axis."
}], "Problem",
 CellChangeTimes->{{3.476968884599695*^9, 3.476968929971706*^9}}],

